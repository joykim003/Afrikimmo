{
    "builds": [
        {
        "src": "realestate_backend/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "50mb", 
            "runtime": "python3.13"
        }}
    
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/realestate_backend/wsgi.py"
        }
    ]
}